{
  "functions": {
    "file-read": [
      {
        "description": "",
        "code": "LFILE=file_to_read\narp -v -f \"$LFILE\"\n"
      }
    ],
    "suid": [
      {
        "description": "",
        "code": "LFILE=file_to_read\n./arp -v -f \"$LFILE\"\n"
      }
    ],
    "sudo": [
      {
        "description": "",
        "code": "LFILE=file_to_read\nsudo arp -v -f \"$LFILE\"\n"
      }
    ]
  }
}
