{
  "functions": {
    "file-read": [
      {
        "description": "",
        "code": "LFILE=file_to_read\nbc -s $LFILE\nquit\n"
      }
    ],
    "sudo": [
      {
        "description": "",
        "code": "LFILE=file_to_read\nsudo bc -s $LFILE\nquit\n"
      }
    ],
    "suid": [
      {
        "description": "",
        "code": "LFILE=file_to_read\n./bc -s $LFILE\nquit\n"
      }
    ]
  }
}
