      {
        "description": "",
        "code": "LFILE=file_to_read\nmsgcat -P $LFILE\n"
      }
    ],
    "sudo": [
      {
        "description": "",
        "code": "LFILE=file_to_read\nsudo msgcat -P $LFILE\n"
      }
    ],
    "suid": [
      {
        "description": "",
