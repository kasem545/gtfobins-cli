{
  "functions": {
    "file-read": [
      {
        "description": "",
        "code": "LFILE=file_to_read\ntac -s 'RANDOM' \"$LFILE\"\n"
      }
    ],
    "suid": [
      {
        "description": "",
        "code": "LFILE=file_to_read\n./tac -s 'RANDOM' \"$LFILE\"\n"
      }
    ],
    "sudo": [
      {
        "description": "",
        "code": "LFILE=file_to_read\nsudo tac -s 'RANDOM' \"$LFILE\"\n"
      }
    ]
  }
}
