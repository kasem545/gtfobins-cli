{
  "functions": {
    "file-read": [
      {
        "description": "",
        "code": "LFILE=file_to_read\nxmore $LFILE\n"
      }
    ],
    "suid": [
      {
        "description": "",
        "code": "LFILE=file_to_read\n./xmore $LFILE\n"
      }
    ],
    "sudo": [
      {
        "description": "",
        "code": "LFILE=file_to_read\nsudo xmore $LFILE\n"
      }
    ]
  }
}
